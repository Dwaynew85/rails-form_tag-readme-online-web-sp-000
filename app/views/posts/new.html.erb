<h3>Post Form</h3>

<form action="<%= posts_path %>" method="post">
  <label>Post title:</label><br>
  <input type="text" id="post_title" name="post[title]"><br>

  <label>Post description</label><br>
  <textarea id="post_description" name="post[description]" rows="8" cols="80"></textarea><br>

  <input type="hiddent" name="authenticity_token" value="<%= form_authenticity_token %>">
  <input type="submit" value="Submit Post">
</form>

<% params.inspect %>
